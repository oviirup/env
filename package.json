{
  "name": "@oviirup/envalid",
  "version": "0.1.3",
  "description": "Validate environment variables with zod in Next.JS",
  "repository": {
    "type": "git",
    "url": "https://github.com/oviirup/envalid"
  },
  "author": "Avirup Ghosh",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./presets": {
      "types": "./dist/presets.d.ts",
      "default": "./dist/presets.js",
      "import": "./dist/presets.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "zod",
    "env validation"
  ],
  "scripts": {
    "build": "bunchee -m",
    "dev": "bunchee -w --no-clean",
    "format": "prettier . -w -u --cache"
  },
  "peerDependencies": {
    "zod": " >=4"
  },
  "devDependencies": {
    "@changesets/changelog-github": "~0.5.1",
    "@changesets/cli": "~2.29.7",
    "@types/node": "~24.7.0",
    "bunchee": "~6.6.0",
    "picocolors": "~1.1.1",
    "prettier": "~3.6.2",
    "prettier-plugin-jsdoc": "~1.3.3",
    "prettier-plugin-pkg": "~0.21.2",
    "typescript": "~5.9.3",
    "zod": "~4.1.12"
  }
}
